{
	"compilerOptions": {
		"noEmit": true,
		"allowJs": true,
		"checkJs": true,
		"strict": true,
		"target": "ESNext",
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"sourceMap": true,
		"skipLibCheck": true,
		"outDir": "dist"
	}
}
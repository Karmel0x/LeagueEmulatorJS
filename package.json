{
    "name": "leagueemulatorjs",
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "devDependencies": {
        "@eslint/js": "^9.31.0",
        "@types/node": "^24",
        "eslint": "^9.31.0",
        "globals": "^16.3.0",
        "typescript-eslint": "^8.37.0"
    },
    "scripts": {
        "dev": "cd apps && cd gameserver && ts-node -T src/main",
        "pi": "cd apps && cd tools && ts-node -T packet-inspector",
        "check-types": "cd apps && cd gameserver && tsc --noEmit"
    }
}